	autoStash = false
	autosquash = false
	updateRefs = false
[difftool "kdiff3"]
	cmd = \"\" \"$LOCAL\" \"$REMOTE\"
[mergetool "kdiff3"]
	cmd = \"\" \"$BASE\" \"$LOCAL\" \"$REMOTE\" -o \"$MERGED\"
